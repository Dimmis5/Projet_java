<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<ScrollPane fx:id="scrollPane" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.projet_java.controller.EdtEtudiantController">
    <content>
        <VBox fx:id="layout" spacing="20" alignment="TOP_CENTER" padding="20">
            <Label fx:id="titreSemaine"/>
            <GridPane fx:id="agendaGrid" hgap="10" vgap="10" padding="10"/>

            <VBox spacing="10">
                <HBox spacing="10">
                    <Label text="Date :"/>
                    <TextField fx:id="text_date"/>
                </HBox>
                <HBox spacing="10">
                    <Label text="Message :"/>
                    <TextField fx:id="text_message"/>
                </HBox>
            </VBox>
        </VBox>
    </content>
</ScrollPane>
